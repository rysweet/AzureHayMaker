# Azure HayMaker Configuration (.env.example)
# Copy this to .env and fill in your values for local development
# DO NOT commit .env to git - it contains secrets!

# Azure Tenant Configuration
AZURE_TENANT_ID=your-tenant-id-here
AZURE_SUBSCRIPTION_ID=your-subscription-id-here
AZURE_CLIENT_ID=your-client-id-here

# Key Vault Configuration
KEY_VAULT_URL=https://your-keyvault.vault.azure.net

# Service Bus Configuration
SERVICE_BUS_NAMESPACE=your-servicebus-namespace
SERVICE_BUS_TOPIC=agent-logs

# Container Configuration
CONTAINER_REGISTRY=your-registry.azurecr.io
CONTAINER_IMAGE=haymaker-agent:latest

# Simulation Configuration
SIMULATION_SIZE=small

# Storage Configuration
STORAGE_ACCOUNT_NAME=your-storage-account
TABLE_STORAGE_ACCOUNT_NAME=your-table-storage-account

# Cosmos DB Configuration
COSMOSDB_ENDPOINT=https://your-cosmosdb.documents.azure.com:443/
COSMOSDB_DATABASE=haymaker

# Log Analytics Configuration
LOG_ANALYTICS_WORKSPACE_ID=your-workspace-id

# Optional: Resource Group Configuration
# RESOURCE_GROUP_NAME=azure-haymaker-rg

# Optional: VNet Configuration
VNET_INTEGRATION_ENABLED=false
# VNET_RESOURCE_GROUP=your-vnet-rg
# VNET_NAME=your-vnet
# SUBNET_NAME=your-subnet

# Secrets (for local dev only - use Key Vault in production!)
# These would normally come from Key Vault
# WARNING: Never commit actual secret values to git!
# MAIN_SP_CLIENT_SECRET=your-secret-here
# ANTHROPIC_API_KEY=your-api-key-here
# LOG_ANALYTICS_WORKSPACE_KEY=your-workspace-key-here
